    <br /> <br />
    <div class="text-center">
    <h2>All Charlottesville Volunteer Entries</h2>
    <br />
    <br />
      <% @entries.each do |entry| %>
        <h3>Title: </h3><%= entry.title %>
        <h3>Location: </h3><%= entry.location %>
        <h3>Date: </h3><%= entry.date %>
        <h3>Description: </h3><%= entry.description %>
        <a href="/entries/<%=entry.id%>/edit">Edit</a><br />
        <br />
        <br />
      <% end %> 
    </div>

   